<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.events.SearchIconActivity">
    <com.love.jax.view.search.SearchIconView
        android:id="@+id/sv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

    <TextView
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_dp_20dp"
        android:layout_alignBottom="@+id/sv"
        android:layout_alignLeft="@+id/sv"
        android:onClick="start"
        android:text="start"
        tools:ignore="OnClick" />

    <TextView
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/sv"
        android:layout_alignRight="@+id/sv"
        android:layout_margin="@dimen/dimen_dp_20dp"
        android:onClick="reset"
        android:text="reset"
        tools:ignore="OnClick" />
</RelativeLayout>